/* pages/store/store.wxss */
page{
  width: 100%;
  min-height: 100%;
  background: #FAFAFA;
}
.nav{
  width: 590rpx;
  overflow: hidden;
  margin: 0 auto;
  padding: 0 40rpx;
  margin-top: 42rpx;
}
.nav_list{
  width: 145rpx;
  float: left;
  text-align: center;
  background: #fff;
  border: solid 1rpx #6C79FB;
  border-right: none;
  border-bottom: none;
  border-radius: 16rpx;
  border-bottom-left-radius:0rpx;
}
.nav_list:last-child{
  border-right: solid 1rpx #6C79FB;
}
.nav_list.cli{
  border-bottom:solid 1rpx #F1F2FF;
  background: #F1F2FF;
  border-bottom-right-radius:0rpx;
  
}
.nav_name{
  color: #6C79FB;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
}
/* .nav_list.cli .nav_icon{
  width: 50rpx;
  height: 6rpx;
  background:#6C79FB ;
  border-radius: 16rpx;
  margin: 0 auto;
} */
.nav_list.cli .nav_name{
  color: #6C79FB;
  font-size: 30rpx;
}
.detail{
  width: 610rpx;
  margin: 0 auto;
  padding-top: 42rpx;
  overflow: hidden;
  line-height: 44rpx;
  color: #222222;
  font-size: 32rpx;
  font-weight:600;
  padding: 42rpx 30rpx 0 30rpx;
  background: #F1F2FF;
  border: solid 1px #6C79FB;
  border-radius: 20rpx;
  margin-top: -1rpx;
}
.detail_name{
  width: 200rpx;
  float: left;
}
.detail_nav{
  width: 324rpx;
  float: right;
  text-align: right;
  margin-top: 20rpx;
}
.detail_nav text{
  display: inline-block;
  width: 80rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  color: #919BA5;
  font-size: 24rpx;
  border-radius: 29rpx;
  margin-left: 20rpx;
  border: solid 1rpx #D8D8D8;
}
.detail_nav text.cli{
  color: #6C79FB;
  background: #F1F2FF;
  border: solid 1rpx #6C79FB;
}
.detail_tit{
  overflow: hidden;
}
.detail_text{
  width: 610rpx;
  margin: 0 auto;
  clear: both;
  margin-top: 26rpx;
  border-radius:10px;
  margin-bottom: 40rpx;
}
.detail_text_list{
  height: 56rpx;
  line-height: 56rpx;
  margin: 26rpx 0;
  color: #6C79FB;
  font-weight:500;
}
.detail_text_list text{
  display: inline-block;
  font-size: 28rpx;
}
.detail_text_list.header text{
  display: inline-block;
  font-size: 24rpx;
  font-weight:400;
  color:#6C79FB;
}

.detail_text_list text:nth-child(1){
  width: 130rpx;
  height: 56rpx;
  text-indent: 7rpx;
}
.detail_text_list text.cli1{
    background: url(https://ylbb-wxapp.oss-cn-beijing.aliyuncs.com/storeManagement/champion.png) no-repeat;
    background-size: 50rpx 46rpx; 
  float: left;
}
.detail_text_list text.cli2{
    background: url(https://ylbb-wxapp.oss-cn-beijing.aliyuncs.com/storeManagement/second.png) no-repeat;
    background-size: 50rpx 46rpx; 
    float: left;
}
.detail_text_list text.cli3{
    background: url(https://ylbb-wxapp.oss-cn-beijing.aliyuncs.com/storeManagement/third.png) no-repeat;
    background-size: 50rpx 46rpx; 
  float: left;
}
/* .detail_text_list text:nth-child(1) image{
  width:50rpx;
  height: 46rpx;
} */

.detail_text_list text:nth-child(2){
  width: 384rpx;
  
}
.detail_text_list text:nth-child(3){
  width: 96rpx;
  text-align: center;
}
.detail_text_list.header text:nth-child(1){
  font-weight:normal;
  color:#6C79FB;
}
.detail_text_list text:nth-child(1){
  font-weight:500;
  color:#6C79FB;
}
.empty{
  text-align: center;
  font-size: 24rpx;
  margin: 150rpx 0 170rpx 0;
  color: #6C79FB;
  font-weight: normal;
}

.headerNav{
  width: 100%;
  overflow: hidden;
  background: #fff;
}
.navList{
  width: 50%;
  height: 104rpx;
  line-height: 104rpx;
  float: left;
  text-align: center;
  font-size: 28rpx;
  color: #919BA5;
  position: relative;
  font-weight: bold;
}
.navList.cli view:last-child{
  background: #6C79FB;
  position: absolute;
  bottom: 0;
  width:30rpx;
  height: 6rpx;
  left: 50%;
  border-radius: 4rpx;
  margin-left: -15rpx;
}
.navList.cli{
  color: #6C79FB;

}

.memberList .headerNav{
  width: 670rpx;
  margin: 0 auto;
  padding: 0 40rpx;
}
.memberList .navList{
  width: 180rpx;
  float: left;
  text-align: left;
  position: relative;
   
}

.memberList .navList.cli view:last-child{
  background: #6C79FB;
  position: absolute;
  bottom: 0;
  width:30rpx;
  height: 6rpx;
  left: 0;
  border-radius: 4rpx;
  margin-left: 0;
 
}


.index-html{
    width: 670rpx;
  overflow: hidden;
  margin: 0 auto;
}
.total{

  overflow: hidden;
  margin-top: 40rpx;
}
.total_tit{
  color: #222;
  font-weight: bold;
  font-size: 32rpx;
  float: left;
  overflow: hidden;
}
.total_top{
  overflow: hidden;
}
.total_icon{
  width: 35rpx;
  height: 35rpx;
  line-height: 35rpx;
  color: #bbb;
  float: left;
  font-size: 30rpx;
  border: solid 1rpx #bbb;
  border-radius: 20rpx;
  text-align: center;
  margin-left: 10rpx;
  margin-top: 5rpx;
  overflow: hidden;

}
.total_list{
  width: 100%;
  clear: both;
  height: 234rpx;
  overflow-x: scroll;
  margin-top: 20rpx;
}


/* 老师模块 */
.teacher{
  margin-top: 40rpx;
  padding-bottom: 30rpx;
}
.teacher_tit{
  color: #222;
  font-weight: bold;
  font-size: 32rpx;
  overflow: hidden;  

}
.teacher_list{
  width: 610rpx;
  height: 134rpx;
  padding: 20rpx 30rpx;
  background: #fff;
  box-shadow:0px 0px 8rpx 0px rgba(221,221,221,1);
  border-radius:16rpx;
  overflow: hidden;
  margin-top: 20rpx;
}
.headImg{
  width: 90rpx;
  height:90rpx;
  border-radius: 50%;
  overflow: hidden;
  float: left;
}
.headImg image{
  width: 90rpx;
  height: 90rpx;
}
.teacher_text{
  width: 500rpx;
  float: left;
  font-size:32rpx;
  font-weight:600;
  color:rgba(34,34,34,1);
  margin-left: 20rpx;
}
.teacher_name{
  width: 350rpx;
  float: left;
  height: 44rpx;
  line-height: 44rpx;
}
.teacher_detail{
  width: 150rpx;
  font-size:28rpx;
  font-weight:500;
  color:rgba(108,121,251,1);
  float: right;
  text-align: right;
}
.text_center{
  clear: both;
  overflow: hidden;
}
.teacher_data{
  font-size:21rpx;
  font-weight:400;
  margin-top: 6rpx;
  color:rgba(153,153,153,1);
  width: 166rpx;
  float: left;
}
.teacher_data text{
  color:#222222;
}
.text_bottom{
  width: 228rpx;
  height: 38rpx;
  color: #fff;
  background: #FF5E86;
  text-align: center;
  border-radius: 20rpx;
  font-size: 24rpx;
  line-height: 38rpx;
  margin-top: 8rpx;
  font-weight: normal;
}
.total_explain{
  width: 610rpx;
  font-size: 24rpx;
  line-height: 34rpx;
  color: #999999;
  overflow: hidden;
  margin: 40rpx 0;
  padding: 30rpx;
  background: #F5F5F5;
  border-radius: 15rpx;
}

.total_explain text{
    float: left;
}
.total_explain text:nth-child(1){
    width: auto;
    color: #222;
}
.total_explain text:nth-child(2){
    width: 510rpx;
}

.total_explain view{
  margin-bottom: 10rpx;
}
.text_footer{
  font-size:24rpx ;
  color: #999999;
  height: 34rpx;
  line-height: 34rpx;
  font-weight: normal;
}
.navigator-hover {
  color: rgba(108,121,251,1);
  background: none;
}

.slist{
  float: left;
  width: 333rpx;
  background-size:100% 100%; 
  height: 234rpx;
  padding: 0 28rpx;
  margin-right: 20rpx;
  border-radius:16px;
  color: #fff;
}
.slist:last-child{
  padding-right: 0;
}
.slist:nth-child(1){
  background: #6C79FB;
}
.slist:nth-child(2){
  background: #FF5E86;
}
.slist:nth-child(3){
  background: #515F71;
}
.slist:nth-child(4){
  background: #FFBD2D;
}
.slist:nth-child(5){
  background: #6C79FB;
}




.slist text:nth-child(1){
  text-align: left;
  font-size: 24rpx;
  margin-top: 44rpx;
  width: 100%;
  display: block;
}
.slist text:nth-child(2){
  width: 100%;
  text-align: left;

  font-size: 24rpx;
  margin-top: 2rpx;
  display: block;
}
.slist text:nth-child(3){
  width: 100%;
  text-align: left;
  font-size: 36rpx;
  margin-top: 20rpx;
  display: block;
  height: 50rpx;
  line-height: 50rpx;
}

