page{
  width: 100%;
  background: #FAFAFA;
}
.messageList{
  width:100%;
  margin-top: 40rpx;
}
.messageDate{
  width: 100%;
  text-align: center;
  color: #9B9B9B;
  font-size: 24rpx;
  height: 34rpx;
  line-height: 34rpx;
}
.messageDetail{
  width: 610rpx;
  overflow: hidden;
  padding: 30rpx;
  margin: 0 auto;
  background: #fff;
  box-shadow:0px 0px 8px 0px rgba(221,221,221,1);
  border-radius:10px;
  margin-top: 20rpx;
}
.messageTitle{
  height:44rpx;
  line-height: 44rpx;
  font-size:32rpx;
  font-weight:600;
  color:rgba(34,34,34,1);
}
.messageText{
  margin-top: 10rpx;
  font-size:28rpx;
  font-weight:400;
  color:rgba(34,34,34,1);
  line-height:40rpx;
}