<view class='content'>
  <view class='header'>
      <view class='usertop'>
            <view class='userBanner'><image src='{{ storeDetail.shopCoverImage ? storeDetail.shopCoverImage : "https://ylbb-wxapp.oss-cn-beijing.aliyuncs.com/storeManagement/userbg1234554565.png" }}'></image></view>
            <view class='store_detail'>
                  <text class='store_name'>{{ storeDetail.shopName }}</text>
                  <text>开业时间：{{ storeDetail.createTime ? storeDetail.createTime :'' }} </text>
                  <text>营业时间：{{ storeDetail.businessTime }} </text>
                  <text>联系电话：{{ storeDetail.shopTel }} </text>
                  <text>地址：{{ storeDetail.shopAddress }}</text>
            </view>
      </view>
 
  </view>

  <view class='storeNum'>
      <view>
          <text>{{ storeDetail.allMemNum  }}</text>
          <text>客户</text>
      </view> 
      <view>
          <text>{{ storeDetail.effectiveNum  }}</text>
          <text>有效会员</text>
      </view> 
      <view>
          <text>{{ storeDetail.overdueNum  }}</text>
          <text>无效会员</text>
      </view> 
      <view>
          <text>{{ storeDetail.employeeNum  }}</text>
          <text>老师</text>
      </view>                   
  </view>  

   <view class='newList'>
      <view bindtap='toCash'>
          <text>管家钱包</text>
          <image src='https://ylbb-wxapp.oss-cn-beijing.aliyuncs.com/storeManagement/next.png'></image>
      </view>    
  </view> 
  <view class='exit' bindtap='exit'>
      退  出
  </view>
</view>
