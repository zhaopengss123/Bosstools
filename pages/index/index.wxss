/**index.wxss**/
page{
  width: 100%;
  min-height: 100%;
  background: #FAFAFA;

}
.content{
  overflow: hidden;
  margin: 0 auto;
}
image{
  width: 100%;
  height: 100%;
}
.banner{
  width: 100%;
  border-radius: 20rpx;
  overflow: hidden;
  background: #fff;
}
.banner image{
  width: 670rpx;
  height: 284rpx;
  margin: 0 auto;
  display: block;
}
.total{
   width: 670rpx;
  margin: 0 auto; 
  overflow: hidden;
  margin-top: 40rpx;
}
.total_tit{
  color: #222;
  font-weight: bold;
  font-size: 32rpx;
  float: left;
  overflow: hidden;
}
.total_top{
  overflow: hidden;
}
.total_icon{
  width: 35rpx;
  height: 35rpx;
  line-height: 35rpx;
  color: #bbb;
  font-size: 30rpx;
  border: solid 1rpx #bbb;
  border-radius: 20rpx;
  text-align: center;
  float: left;
  margin-top: 5rpx;
  overflow: hidden;
  margin-left: 10rpx;
}
.total_list{
  width: 100%;
  clear: both;
  height: 234rpx;
  overflow-x: scroll;
  margin-top: 20rpx;
}

.nav{
  width: 100%;
  overflow: hidden;
}
.navList{
  width: 50%;
  height: 104rpx;
  line-height: 104rpx;
  float: left;
  text-align: center;
  font-size: 28rpx;
  color: #919BA5;
  position: relative;
  font-weight: bold;
}
.navList.cli view:last-child{
  background: #6C79FB;
  position: absolute;
  bottom: 0;
  width:30rpx;
  height: 6rpx;
  left: 50%;
  border-radius: 4rpx;
  margin-left: -15rpx;
}
.navList.cli{
  color: #6C79FB;

}



.detail{
  margin-top: 42rpx;
  overflow: hidden;
  line-height: 44rpx;
  color: #222222;
  font-weight:600;
  padding-bottom: 5px;
}
.detail_name{
  width: 200rpx;
  float: left;
  color: #373C41;
  font-size: 32rpx;
  overflow: hidden;
  font-weight: bold;
}
.detail_name text{
  float: left;
}
.detail_nav{
  width: 470rpx;
  float: left;
  text-align: right;
}
.detail_nav text{
  display: inline-block;
  width:auto;
  padding: 0 16rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  color: #919BA5;
  font-weight:500;
  font-size: 24rpx;
  border-radius: 25rpx;
  margin-left: 20rpx;
  border: solid 1px #D8D8D8;
}
.detail_nav text.cli{
  color: #6C79FB;
  background: #F1F2FF;
  border: solid 1px #6C79FB;
}
.detail_tit{
  overflow: hidden;
  width: 670rpx;
  margin: 0 auto;
}


/* 老师模块 */
.teacher{
 width: 670rpx;
  margin: 0 auto; 
  margin-top: 40rpx;
  padding-bottom: 30rpx;
}
.teacher_tit{
  color: #222;
  font-weight: bold;
  font-size: 32rpx;
  overflow: hidden;  

}
.teacher_list{
  width: 610rpx;
  height: 134rpx;
  padding: 20rpx 30rpx;
  background: #fff;
  box-shadow:0px 0px 8rpx 0px rgba(221,221,221,1);
  border-radius:16rpx;
  overflow: hidden;
  margin-top: 20rpx;
}
.headImg{
  width: 90rpx;
  height:90rpx;
  border-radius: 50%;
  overflow: hidden;
  float: left;
}
.teacher_text{
  width: 500rpx;
  float: left;
  font-size:32rpx;
  font-weight:600;
  color:rgba(34,34,34,1);
  margin-left: 20rpx;
}
.teacher_name{
  width: 350rpx;
  float: left;
  height: 44rpx;
  line-height: 44rpx;
}
.teacher_detail{
  width: 150rpx;
  font-size:28rpx;
  font-weight:500;
  color:rgba(108,121,251,1);
  float: right;
  text-align: right;
}
.text_center{
  clear: both;
  overflow: hidden;
}
.teacher_data{
  font-size:21rpx;
  font-weight:400;
  margin-top: 6rpx;
  color:rgba(153,153,153,1);
  width: 166rpx;
  float: left;
}
.teacher_data text{
  color:#222222;
}
.text_bottom{
  width: 228rpx;
  height: 38rpx;
  color: #fff;
  background: #FF5E86;
  text-align: center;
  border-radius: 20rpx;
  font-size: 24rpx;
  line-height: 38rpx;
  margin-top: 8rpx;
  font-weight: normal;
}
.total_explain{
  width: 610rpx;
  font-size: 24rpx;
  line-height: 34rpx;
  color: #999999;
  overflow: hidden;
  margin: 40rpx 0;
  padding: 30rpx;
  background: #F5F5F5;
  border-radius: 15rpx;
}

.total_explain text{
    float: left;
}
.total_explain text:nth-child(1){
    width: auto;
    color: #222;
}
.total_explain text:nth-child(2){
    width: 510rpx;
}

.total_explain view{
  margin-bottom: 10rpx;
}
.text_footer{
  font-size:24rpx ;
  color: #999999;
  height: 34rpx;
  line-height: 34rpx;
  font-weight: normal;
}
.navigator-hover {
  color: rgba(108,121,251,1);
  background: none;
}

.slist{
  float: left;
  width: 336rpx;
  background: url(https://ylbb-wxapp.oss-cn-beijing.aliyuncs.com/storeManagement/bg_dsadsad.png) no-repeat;
  background-size:100% 100%; 
  height: 234rpx;
  padding: 0 28rpx;
  margin-right: 20rpx;
}
.slist:nth-child(4){
  padding-right: 0;
}
.slist text:nth-child(1){
  text-align: left;
  color: #222;
  font-size: 24rpx;
  margin-top: 44rpx;
  width: 100%;
  display: block;
}
.slist text:nth-child(2){
  width: 100%;
  text-align: left;
  color: #999;
  font-size: 24rpx;
  margin-top: 2rpx;
  display: block;
}
.slist text:nth-child(3){
  width: 100%;
  text-align: left;
  color: #6C79FB;
  font-size: 36rpx;
  margin-top: 20rpx;
  display: block;
  height: 50rpx;
  line-height: 50rpx;
}

.ts{
  width:626rpx;
  overflow: hidden;
  padding: 20rpx;
  margin: 0 auto;
  background: #F0F0F0;
  border: solid 1px #D8D8D8;
  color: #919BA5;
  font-size: 24rpx;
  font-weight: normal;
  border-radius:16rpx;
}
.selectDate{
   width: 670rpx;
  margin: 0 auto; 
  height: 54rpx;
  line-height: 54rpx;
  color: #919BA5;
  font-size: 24rpx;
  font-weight: normal;
}
.selectDate view{
  width: auto;
  float: left;
  margin-right: 10rpx;
}
.sefont{
  color: #373C41;
}

.amount{
  width: 670rpx;
  margin: 0 auto; 
  background:rgba(250,250,250,1);
  box-shadow:0px 0px 8rpx 0px rgba(221,221,221,1);
  overflow:hidden;
  border-radius: 16rpx;
  padding-bottom: 30rpx;
  margin-top: 20rpx;
}
.amountTit{
  width: 630rpx;
  padding: 0 20rpx;
  height: 80rpx;
  line-height: 80rpx;
  overflow: hidden;
  background: #fff;
}
.amountTit view:first-child{
  color: #373C41;
  font-size: 24rpx;
  width: auto;
  float: left;
}
.amountTit view:first-child text{
  color: #6C79FB;
  display: inline-block;
  font-size: 28rpx;
  margin-left: 20rpx;
}
.amountTit view:last-child{
  float: right;
  color: #C3CDD7;
  font-size: 22rpx;
  font-weight: normal;
  
}

.amountMain{
  width: 630rpx;
  padding: 0 20rpx;
}
.amountList{
  width: 157.5rpx;
  float: left;
  margin-top: 20rpx;
  font-weight: normal;
}
.amountList view:nth-child(1){
  color: #AFB9C3;
  font-size: 24rpx;
}
.amountList view:nth-child(2){
  color: #373C41;
  font-size: 28rpx;
  font-weight:600;
  margin-top: 6rpx;
}

.orderList{
  margin-top: 42rpx;
  overflow: hidden;
}
.add_icon.add_icon{
  width: 40rpx;
  height: 40rpx;
  background: url('https://ylbb-wxapp.oss-cn-beijing.aliyuncs.com/storeManagement/addIcons.png') no-repeat;
  background-size:100% 100%; 
  display: inline-block;
  margin-left: 12rpx;
}
.detail_name{
  margin-top: 10rpx;
}

.order_list{
  width: 670rpx;
  margin: 0 auto;
  background:rgba(255,254,254,1);
  box-shadow:0px 0px 8rpx 0px rgba(221,221,221,1);
  border-radius:16rpx;  
  overflow: hidden;
  margin-top: 20rpx;
}
.order_img {
  width: 90rpx;
  height: 90rpx;
  float: left;
  margin: 20rpx 0 0 30rpx;
  position: relative;
}
.order_img image{
  width: 100%;
  height: 100%;
    border-radius: 50%;
}
.order_detail{
  width: 510rpx;
  float: left;
}
.order_tit{
  width: 350rpx;
  float: left;
  overflow: hidden;
  margin-left: 20rpx;
  margin-top: 20rpx;
}
.order_name{
  font-size: 32rpx;
  color: #373C41;
  font-weight: bold;
}
.order_label{
  margin-top: 6rpx;
  width:318rpx;
  overflow: hidden;
}
.order_label text{
  font-size: 24rpx;
  color: #919BA5;
  background: #F5F5F5;
  padding: 6rpx 16rpx;
  border-radius: 23rpx;
  margin-right: 10rpx;
}
.order_btn{
  width: 128rpx;
  color: #fff;
  font-size: 24rpx;
  background: #6C79FB;
  float: right;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  margin-top: 20rpx;
  border-radius:12rpx;
}
.memberStatus{
  width: 90rpx;
  margin-left: 30rpx;
  float: left;
  font-size: 24rpx;
  color: #919BA5;
  height: 63rpx;
  line-height: 63  rpx;
}
.card_number{
  float: left;
}
.card_number view{
  display: inline-block;
  color: #919BA5;
  font-size: 24rpx;
  margin-right: 20rpx;
}
.card_number text{
  display: inline-block;
  margin-left: 10rpx;
  color: #373C41;
}
.order_teacher{
  width: 500rpx;
  margin-left: 140rpx;
  border-top: solid 1px #F0F0F0;
  margin-top: 20rpx;
  padding: 18rpx 0;
  overflow: hidden;
}
.orderMain{
  overflow: hidden;
  padding-bottom: 20rpx;
}
.teacher_img{
  width: 60rpx;
  height: 60rpx;
  float: left;
  border-radius: 50%;
  overflow: hidden;
}
.teacher_name{
  height: 60rpx;
  line-height: 60rpx;
  float: left;
  margin-left: 20rpx;
  width: auto;
  color: #373C41;
  font-size: 24rpx;

}
.teacher_date{
  height: 60rpx;
  line-height: 60rpx;
  float: right;
  width: auto;
  color: #919BA5;
  font-size: 24rpx;  
}
.order_img view{
  width: 38rpx;
  height: 38rpx;
  position: absolute;
  bottom: -5rpx;
  right: -5rpx;
}

.noMemberList{
  width: 100%;
}
.noMemberList image{
  width: 332rpx;
  height: 332rpx;
  display: block;
  margin: 0 auto;
  margin-top: 84rpx;
}
.noMemberList view{
  width: 100%;
  text-align: center;
  color: #C3CDD7;
  font-size: 24rpx;
}
.qxbtn{
  width:128rpx;
  color: #6C79FB;
  font-size: 24rpx;
